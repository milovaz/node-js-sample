<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest  ApplicationTypeName="nodeSampleType" ApplicationTypeVersion="2.0.0"
                      xmlns="http://schemas.microsoft.com/2011/01/fabric" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   
   <ServiceManifestImport>
      <ServiceManifestRef ServiceManifestName="nodeSampleServicePkg" ServiceManifestVersion="1.0.0" />
        <Policies>
            <ContainerHostPolicies CodePackageRef="Code">
                <PortBinding ContainerPort="5000" EndpointRef="nodeSampleServiceEndpoint"/>
            </ContainerHostPolicies>
        </Policies>
   </ServiceManifestImport>
   
   <DefaultServices>
      <Service Name="nodeSampleService" ServiceDnsName="nodesample.api">
         <StatelessService ServiceTypeName="nodeSampleServiceType" InstanceCount="1">
            <SingletonPartition />
         </StatelessService>
      </Service>
   </DefaultServices>
   
</ApplicationManifest>
